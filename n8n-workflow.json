      },
      "name": "Schedule - Preview (9 PM)",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [250, 1100],
      "id": "schedule-preview"
    },
    {
      "parameters": {
        "command": "cd /home/gameofcrores && node src/scripts/createVideo.js market_open"
      },
      "name": "Execute - Market Open",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [500, 300],
      "id": "exec-market-open"
    },
    {
